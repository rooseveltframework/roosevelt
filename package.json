{
  "name": "roosevelt",
  "description": "Roosevelt MVC web framework",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.11.9",
  "homepage": "https://github.com/rooseveltframework/roosevelt",
  "license": "CC-BY-4.0",
  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "app-module-path": "~2.2.0",
    "body-parser": "~1.18.0",
    "browserify": "~16.2.0",
    "check-dependencies": "~1.1.0",
    "colors": "~1.3.0",
    "compression": "~1.7.0",
    "cookie-parser": "~1.4.0",
    "es6-template-strings": "~2.0.1",
    "express": "~4.16.2",
    "fkill": "~5.3.0",
    "formidable": "~1.2.0",
    "fs-extra": "~7.0.0",
    "html-minifier": "~3.5.15",
    "html-validator": "~3.0.0",
    "klaw": "~2.1.1",
    "klaw-sync": "~4.0.0",
    "method-override": "~3.0.0",
    "morgan": "~1.9.0",
    "node-emoji": "~1.8.1",
    "parent-require": "~1.0.0",
    "ps-node": "~0.1.6",
    "serve-favicon": "~2.5.0",
    "tamper": "~1.0.0",
    "tmp": "~0.0.33",
    "toobusy-js": "~0.5.0",
    "vnu-jar": "~18.7.0"
  },
  "devDependencies": {
    "codecov": "3.0.4",
    "eslint": "4.19.1",
    "eslint-plugin-mocha": "5.1.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "proxyquire": "2.0.1",
    "roosevelt-less": "0.5.0",
    "roosevelt-uglify": "0.3.0",
    "sinon": "6.1.3",
    "standard": "11.0.1",
    "supertest": "3.1.0",
    "teddy": "0.4.25"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc"
  ],
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "coverage": "nyc mocha --timeout 125000 test/unit",
    "lint": "standard && eslint ./test/unit",
    "postinstall": "node ./lib/scripts/configAuditor.js",
    "precommit": "lint-staged",
    "test": "npm run lint && mocha --timeout 125000 test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  }
}
