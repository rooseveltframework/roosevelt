    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc",
    "Express",
    "Express.js",
    "framework",
    "web framework"
  ],
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "mocha": {
    "timeout": "60s"
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha test/*.js",
    "lint": "standard && eslint test/*.js",
    "test": "mocha test/*.js",
    "test-debug": "mocha --inspect-brk test/*.js"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/kethinov"
  }
