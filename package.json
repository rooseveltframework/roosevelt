{
  "name": "roosevelt",
  "description": "Roosevelt MVC web framework",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/roosevelt/graphs/contributors"
    }
  ],
  "version": "0.18.3",
  "files": [
    "defaultErrorPages",
    "lib",
    "roosevelt.js"
  ],
  "homepage": "https://github.com/rooseveltframework/roosevelt",
  "license": "CC-BY-4.0",
  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=12.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "app-module-path": "~2.2.0",
    "clean-css": "~4.2.3",
    "colors": "~1.4.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.5",
    "es6-template-strings": "~2.0.1",
    "express": "~4.17.1",
    "formidable": "~1.2.2",
    "fs-extra": "~9.0.1",
    "helmet": "~4.0.0",
    "html-minifier": "~4.0.0",
    "klaw": "~3.0.0",
    "klaw-sync": "~6.0.0",
    "method-override": "~3.0.0",
    "morgan": "~1.10.0",
    "parent-require": "~1.0.0",
    "roosevelt-logger": "~0.2.2",
    "serve-favicon": "~2.5.0",
    "source-configs": "~0.3.3",
    "toobusy-js": "~0.5.1",
    "webpack": "~4.44.0"
  },
  "devDependencies": {
    "c8": "~7.2.1",
    "codecov": "~3.7.1",
    "eslint": "~7.5.0",
    "eslint-plugin-mocha": "~7.0.1",
    "husky": "~4.2.5",
    "less": "~3.12.2",
    "lint-staged": "~10.2.11",
    "mocha": "~8.0.1",
    "node-sass": "~4.14.1",
    "proxyquire": "~2.1.3",
    "sinon": "~9.0.2",
    "standard": "~14.3.4",
    "stylus": "~0.54.8",
    "supertest": "~4.0.2",
    "teddy": "~0.5.3"
  },
  "optionalDependencies": {
    "check-dependencies": "~1.1.0",
    "express-html-validator": "~0.1.2",
    "reload": "~3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc",
    "Express",
    "Express.js",
    "framework",
    "web framework"
  ],
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha --timeout 60000 test/*.js",
    "lint": "standard && eslint test/*.js",
    "postinstall": "node ./lib/scripts/configAuditor.js && node ./lib/scripts/deprecationCheck.js",
    "test": "mocha --timeout 60000 test/*.js",
    "test-debug": "mocha --inspect-brk --timeout 60000 test/*.js"
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint test/*.js"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/kethinov"
  }
}
