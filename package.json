{
  "name": "roosevelt",
  "description": "Roosevelt MVC web framework",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.11.9",
  "homepage": "https://github.com/rooseveltframework/roosevelt",
  "license": "CC-BY-4.0",
  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "app-module-path": "~2.2.0",
    "body-parser": "~1.18.0",
    "browserify": "~16.2.0",
    "colors": "~1.2.0",
    "compression": "~1.7.0",
    "cookie-parser": "~1.4.0",
    "es6-template-strings": "~2.0.1",
    "express": "~4.16.2",
    "express-minify-html": "~0.12.0",
    "formidable": "~1.2.0",
    "fs-extra": "~6.0.0",
    "html-validator": "~3.0.0",
    "klaw-sync": "~4.0.0",
    "method-override": "~2.3.0",
    "morgan": "~1.9.0",
    "node-emoji": "~1.8.1",
    "parent-require": "~1.0.0",
    "serve-favicon": "~2.5.0",
    "tamper": "~1.0.0",
    "toobusy-js": "~0.5.0",
    "fkill": "~5.3.0",
    "vnu-jar": "~18.3.0"
  },
  "devDependencies": {
    "coveralls": "~3.0.0",
    "eslint": "~4.19.0",
    "eslint-plugin-no-only-tests": "~2.0.0",
    "husky": "~0.14.2",
    "lint-staged": "~7.0.0",
    "mocha": "~5.1.0",
    "nyc": "~11.7.1",
    "proxyquire": "~2.0.0",
    "roosevelt-less": "~0.5.0",
    "roosevelt-uglify": "~0.2.2",
    "sinon": "~5.0.0",
    "standard": "~11.0.0",
    "supertest": "~3.0.0",
    "teddy": "~0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc"
  ],
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "scripts": {
    "postinstall": "node ./lib/scripts/configAuditor.js",
    "coverage": "nyc --reporter=html --reporter=text mocha --timeout 125000 --recursive test/unit",
    "coveralls": "nyc --reporter=lcov mocha --timeout 125000 --recursive test && cat ./coverage/lcov.info | coveralls",
    "standard": "standard",
    "test": "mocha --timeout 125000 --recursive test/unit && npm run standard",
    "precommit": "lint-staged && eslint test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  }
}
