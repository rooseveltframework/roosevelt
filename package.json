{
  "name": "roosevelt",
  "description": "Roosevelt MVC web framework",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/roosevelt/graphs/contributors"
    }
  ],
  "version": "0.15.1",
  "files": [
    "defaultErrorPages",
    "lib",
    "roosevelt.js"
  ],
  "homepage": "https://github.com/rooseveltframework/roosevelt",
  "license": "CC-BY-4.0",
  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "app-module-path": "~2.2.0",
    "body-parser": "~1.19.0",
    "browserify": "~16.5.0",
    "check-dependencies": "~1.1.0",
    "clean-css": "~4.2.1",
    "colors": "~1.4.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "es6-template-strings": "~2.0.1",
    "express": "~4.17.1",
    "formidable": "~1.2.1",
    "fkill": "~6.2.0",
    "fs-extra": "~8.1.0",
    "html-minifier": "~4.0.0",
    "html-validator": "~5.0.0",
    "klaw": "~3.0.0",
    "klaw-sync": "~6.0.0",
    "method-override": "~3.0.0",
    "morgan": "~1.9.1",
    "parent-require": "~1.0.0",
    "prismjs": "~1.17.1",
    "ps-node": "~0.1.6",
    "reload": "~3.0.3",
    "roosevelt-logger": "~0.2.2",
    "serve-favicon": "~2.5.0",
    "source-configs": "~0.2.1",
    "tamper": "~1.1.0",
    "tmp": "~0.1.0",
    "toobusy-js": "~0.5.1",
    "vnu-jar": "~19.9.4"
  },
  "devDependencies": {
    "c8": "~6.0.1",
    "codecov": "~3.6.1",
    "eslint": "~6.7.1",
    "eslint-plugin-mocha": "~6.2.2",
    "husky": "~3.1.0",
    "lint-staged": "~9.5.0",
    "mocha": "~6.2.2",
    "proxyquire": "~2.1.3",
    "roosevelt-less": "~0.8.0",
    "roosevelt-uglify": "~0.3.2",
    "sinon": "~7.5.0",
    "standard": "~14.3.1",
    "supertest": "~4.0.2",
    "teddy": "~0.4.28"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc",
    "Express",
    "Express.js",
    "framework",
    "web framework"
  ],
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha --timeout 125000 test/unit",
    "lint": "standard && eslint ./test/unit",
    "postinstall": "node ./lib/scripts/configAuditor.js",
    "test": "mocha --timeout 125000 test/unit",
    "test-debug": "mocha --inspect-brk --timeout 125000 test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint ./test/unit"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/kethinov"
  }
}
